void GAPI_line()
{
	int *cmd == FS:EBX;
	GAPI_WORK *work == ES:0x0000;

	EAX = cmd[4]; /* opt */
	TEST(EAX, 0xffffff0f);
	if (!= 0)
		goto error;

	EAX &= 0x0030;
	if (== 0)
		goto line;

	if (EAX == 0x0020) {
		/* boxfill */
		EBP = cmd[20]; /* y1 */
		ESI = cmd[12]; /* y0 */
		EBP -= ESI;
		IMUL(ESI, work->linebytes);

boxfill:
		EDX = cmd[ 8]; /* x0 */
		EDI = cmd[16]; /* x1 */
	//	ESI += EDX + 0xe0000000;
		LEA(ESI, [ESI + EDX + 0xe0000000]);
		EDI++;
		CL = cmd[ 4]; /* opt */
		AL = cmd[24]; /* color */
		CL &= 0xc0;
		AH = AL;
		EDI -= EDX;
		EDX = (unsigned short) AX;
		EAX <<= 16;
		/* EDI : x方向のサイズ(バイト数) */
		/* EBP : y方向のサイズ(ドット数 - 1) */
		/* ESI : VRAM上のアドレス */
		EBP++;
		EAX |= EDX;
		/* 少し工夫した描画ルーチン */
		/* 縦に描いていく */
		PUSH((offset) cmd);
		if (CL == 0) {
			if ((unsigned) EBP >= 8) {
				TEST(ESI, 0x03);
				if (!= 0) {
					/* 手抜き */
					do {
						ECX = EBP;
						EBX = ESI;
						ECX -= 8;
						if ((unsigned) >= 0) {
							PUSH(EBP);
							EBP = work->linebytes;
							do {
								[DS:ESI +       0] = AL;
								[DS:ESI + EBP * 1] = AL;
								LEA(ESI, [ESI + EBP * 2]);
								[DS:ESI +       0] = AL;
								[DS:ESI + EBP * 1] = AL;
								LEA(ESI, [ESI + EBP * 2]);
								[DS:ESI +       0] = AL;
								[DS:ESI + EBP * 1] = AL;
								LEA(ESI, [ESI + EBP * 2]);
								[DS:ESI +       0] = AL;
								[DS:ESI + EBP * 1] = AL;
								LEA(ESI, [ESI + EBP * 2]);
								ECX -= 8;
