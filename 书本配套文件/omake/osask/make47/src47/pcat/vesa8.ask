				EAX *= 16;
				EDX -= ECX; /* これで13倍 */
				EAX -= EDI;
				POP(EDI);
				DH += 3;
				EAX += 0x03000300;
				DL = DH; /* green */
				ECX = EAX;
				EAX >>= 16; /* AH */
				DH &= 0x03;
				CL = CH; /* blue */
				AL = AH; /* red */
				CH &= 0x03;
				AH &= 0x03;
				DL /= 4;
				AL /= 4;
				CL /= 4;
				CMP(DH, BH);
				SBB(DL, 0);
				CMP(AH, BH);
				SBB(AL, 0);
				CMP(CH, BH);
				SBB(CL, 0);
				DL *= 4;
				CL *= 16;
				AL |= BL;
				DL |= CL;
				AL |= DL;
				BH ^= 0x02;
				*q = AL;
				(offset) q++;
				EBP--;
			} while (!= 0);
			BH ^= 0x01;
			(offset) p += [SS:ESP +  8];
			(offset) q += [SS:ESP + 12];
			(int) [SS:ESP + 4]--; /* xsize */
		} while (!= 0);
		ESP += 16;
		POP((offset) cmd);
		(offset) cmd += 36;
		POP(EBP);
		POP(DS);
		POP(ES);
		asmout("JMP next_command");
	}
	INT(0x03);
putbox1:
	AH = (char) cmd[36];
	do {
		ECX = cmd[16]; /* xsize */
		do {
			AL = *p;
			(offset) p++;
			if (AH != AL)
				*q = AL;
			(offset) q++;
			ECX--;
		} while (!= 0);
		(offset) p += cmd[24]; /* yskip */
		(offset) q += EBP;
		EDX--;
	} while (!= 0);
	(offset) cmd += 40;
	POP(EBP);
	POP(DS);
	POP(ES);
	asmout("JMP next_command");
