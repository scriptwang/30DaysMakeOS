				PUSH(EDI);
				EDX = *pi; /* green */
				(offset) p += 4;
				EAX = EDX; /* blue & red */
				EDX >>= 8;
				EAX &= 0xff00ff;
				EDX &= 0x0000ff;
				BL = 0xc0;
